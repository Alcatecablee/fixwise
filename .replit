modules = ["nodejs-16", "web"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33613
externalPort = 4200

[[ports]]
localPort = 34945
externalPort = 5000

[[ports]]
localPort = 35469
externalPort = 3002

[[ports]]
localPort = 36373
externalPort = 3003

[[ports]]
localPort = 38261
externalPort = 3001

[[ports]]
localPort = 42339
externalPort = 6000

[[ports]]
localPort = 44107
externalPort = 8000

[[ports]]
localPort = 44639
externalPort = 5173

[[ports]]
localPort = 44895
externalPort = 3000

[[ports]]
localPort = 46089
externalPort = 6800

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Landing Page"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "API Server"

[[workflows.workflow]]
name = "Landing Page"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "API Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev:api"
waitForPort = 3001

[workflows.workflow.metadata]
outputType = "console"

[agent]
integrations = ["javascript_websocket:1.0.0", "javascript_mem_db:1.0.0"]
expertMode = true
