{
  "name": "neurolint-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "NeuroLint CLI - Deterministic code fixing for TypeScript, JavaScript, React, and Next.js",
  "main": "cli.js",
  "bin": {
    "neurolint": "cli.js"
  },
  "files": [
    "cli.js",
    "ast-transformer.js",
    "backup-manager.js",
    "backup-manager-production.js",
    "backup-error-handler.js",
    "validator.js",
    "selector.js",
    "simple-ora.js",
    "fix-master.js",
    "scripts/",
    "shared-core/",
    "README.md",
    "CLI_USAGE.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.22",
    "glob": "^12.0.0",
    "ora": "^5.4.1",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^3.4.1",
    "vite": "^4.5.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "cli",
    "code-quality",
    "linter",
    "typescript",
    "javascript",
    "react",
    "nextjs",
    "ast",
    "code-transformation",
    "static-analysis",
    "eslint",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurolint/neurolint-cli.git"
  },
  "bugs": {
    "url": "https://github.com/neurolint/neurolint-cli/issues"
  },
  "homepage": "https://github.com/neurolint/neurolint-cli#readme",
  "license": "MIT",
  "author": "NeuroLint"
